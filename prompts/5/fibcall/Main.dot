digraph Main {
    rankdir=TB;
    node [shape=box];

    // Nos
    start [label="Inicio main()", shape=ellipse];
    decl [label="1: int a"];
    assign [label="2: a = 30"];
    call_fib [label="3: fib(a)"];
    ret [label="4: return a", shape=ellipse];

    // Arestas de controle
    start -> decl;
    decl -> assign;
    assign -> call_fib;
    call_fib -> ret;

    // Arestas de dados
    assign -> call_fib [label="a", style=dashed, color=blue];
    assign -> ret [label="a", style=dashed, color=blue];
}