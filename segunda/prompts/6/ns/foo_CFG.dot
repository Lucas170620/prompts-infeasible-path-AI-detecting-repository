digraph foo_CFG {
    node [shape=box, style=rounded, fontname="Helvetica"];
    entry [label="Entry", shape=ellipse];
    exit [label="Exit", shape=ellipse];

    N0 [label="i=0"];
    N1 [label="i<5"];
    N2 [label="j=0"];
    N3 [label="j<5"];
    N4 [label="k=0"];
    N5 [label="k<5"];
    N6 [label="l=0"];
    N7 [label="l<5"];
    N8 [label="if (keys[i][j][k][l] == x)"];
    N9 [label="return answer[i][j][k][l] + keys[i][j][k][l];"];
    N10 [label="l++"];
    N11 [label="k++"];
    N12 [label="j++"];
    N13 [label="i++"];
    N14 [label="return -1;"];

    entry -> N0;
    N0 -> N1;
    N1 -> N2 [label="true"];
    N1 -> N14 [label="false"];
    N2 -> N3;
    N3 -> N4 [label="true"];
    N3 -> N13 [label="false"];
    N4 -> N5;
    N5 -> N6 [label="true"];
    N5 -> N12 [label="false"];
    N6 -> N7;
    N7 -> N8 [label="true"];
    N7 -> N11 [label="false"];
    N8 -> N9 [label="true"];
    N8 -> N10 [label="false"];
    N9 -> exit;
    N10 -> N7;
    N11 -> N5;
    N12 -> N3;
    N13 -> N1;
    N14 -> exit;
}