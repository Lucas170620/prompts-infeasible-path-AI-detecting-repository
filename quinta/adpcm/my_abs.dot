digraph my_abs {
    rankdir=TB;
    node [shape=box, fontname=Arial];
    edge [fontname=Arial];

    start [label="Start", shape=ellipse];
    end [label="End", shape=ellipse];
    cond1 [label="if (n >= 0)", shape=diamond];
    assign1 [label="m = n"];
    assign2 [label="m = -n"];
    return_stmt [label="return m"];

    start -> cond1;
    cond1 -> assign1 [label="True"];
    cond1 -> assign2 [label="False"];
    assign1 -> return_stmt;
    assign2 -> return_stmt;
    return_stmt -> end;
}