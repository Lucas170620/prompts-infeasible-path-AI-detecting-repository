    N3 -> N4 [label="true"];
    N3 -> N11 [label="false"];
    N4 -> N5;
    N5 -> N6 [label="true"];
    N5 -> N10 [label="false"];
    N6 -> N7 [label="true"];
    N6 -> N8 [label="false"];
