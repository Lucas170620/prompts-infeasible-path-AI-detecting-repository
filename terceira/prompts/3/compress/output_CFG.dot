digraph output_CFG {
    node [shape=box];
    N1 [label="1: if (code >= 0)"];
    N2 [label="2: Processamento para code >= 0"];
    N3 [label="3: if (offset == (n_bits << 3))"];
    N4 [label="4: if (free_ent > maxcode || clear_flg)"];
    N5 [label="5: else (code < 0)"];
    N6 [label="6: End"];
    N1 -> N2 [label="true"];
    N1 -> N5 [label="false"];
    N2 -> N3 -> N4 -> N6;
    N5 -> N6;
}