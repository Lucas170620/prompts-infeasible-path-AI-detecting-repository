digraph my_cos_DFG {
    node [shape=ellipse, style=filled, fillcolor=lightblue];
    rad_in [label="rad"];
    PI_in [label="PI"];

    node [shape=box, style="", fillcolor=""];
    op_sub [label="-"];
    op_div [label="/"];

    node [shape=ellipse, style=filled, fillcolor=lightgreen];
    result [label="return"];

    rad_in -> op_sub;
    PI_in -> op_div;
    op_div -> op_sub;
    op_sub -> result;
}