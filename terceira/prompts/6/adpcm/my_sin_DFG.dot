digraph my_sin_DFG {
    node [shape=ellipse, style=filled, fillcolor=lightblue];
    rad_in [label="rad"];
    PI_in [label="PI"];

    node [shape=box, style=""];
    op_sub_2PI [label="-="];
    op_add_2PI [label="+="];
    op_assign_diff [label="="];
    op_assign_app [label="="];
    op_compute_diff [label="compute_diff"];
    op_inc [label="++"];
    op_fabs [label="my_fabs"];

    node [shape=ellipse, style=filled, fillcolor=lightgreen];
    rad_out [label="rad"];
    diff_out [label="diff"];
    app_out [label="app"];
    inc_out [label="inc"];

    rad_in -> op_sub_2PI;
    PI_in -> op_sub_2PI;
    op_sub_2PI -> rad_out;

    rad_in -> op_add_2PI;
    PI_in -> op_add_2PI;
    op_add_2PI -> rad_out;

    rad_out -> op_assign_diff;
    op_assign_diff -> diff_out;

    diff_out -> op_assign_app;
    op_assign_app -> app_out;

    diff_out -> op_compute_diff;
    rad_out -> op_compute_diff;
    inc_out -> op_compute_diff;
    op_compute_diff -> diff_out;

    diff_out -> op_fabs;
    op_fabs -> N9; // Used in condition

    inc_out -> op_inc;
    op_inc -> inc_out;
}