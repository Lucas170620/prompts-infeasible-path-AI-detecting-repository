digraph fib_CFG {
    node [shape=box, style=rounded, fontname="Helvetica"];
    N0 [label="0: Fnew = 1;\nFold = 0;\ni = 2;"];
    N1 [label="1: if (i <= 30 && i <= n)"];
    N2 [label="2: temp = Fnew;\nFnew = Fnew + Fold;\nFold = temp;\ni++;"];
    N3 [label="3: ans = Fnew;\nreturn ans;"];
    N0 -> N1;
    N1 -> N2 [label="true"];
    N1 -> N3 [label="false"];
    N2 -> N1;
}