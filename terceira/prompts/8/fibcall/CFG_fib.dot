digraph CFG_fib {
    node [shape=box, style=rounded, fontname="Helvetica"];
    N1 [label="1: Fnew = 1; Fold = 0;"];
    N2 [label="2: i = 2;"];
    N3 [label="3: if (i <= 30 && i <= n)"];
    N4 [label="4: temp = Fnew; Fnew = Fnew + Fold; Fold = temp;"];
    N5 [label="5: i++"];
    N6 [label="6: ans = Fnew; return ans;"];
    N1 -> N2;
    N2 -> N3;
    N3 -> N4 [label="true"];
    N3 -> N6 [label="false"];
    N4 -> N5;
    N5 -> N3;
}