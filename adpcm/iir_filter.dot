digraph iir_filter {
    rankdir=TB;
    node [shape=box, fontname=Arial];
    edge [fontname=Arial];

    start [label="Start", shape=ellipse];
    end [label="End", shape=ellipse];
    entry [label="Function Entry"];
    return [label="return input;"];
    
    start -> entry;
    entry -> return [label="Direct path"];
    return -> end;
}